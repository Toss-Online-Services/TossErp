{
  "openapi": "3.0.1",
  "info": { "title": "Orchestrator", "version": "0.1.0" },
  "paths": {
    "/orchestrator/execute": {
      "post": {
        "summary": "Execute a new workflow",
        "requestBody": { "content": { "application/json": { "schema": { "type": "object" } } } },
        "responses": { "200": { "description": "workflow started", "content": { "application/json": { "schema": { "type": "object" } } } } }
      }
    },
    "/orchestrator/workflow/{id}": {
      "get": {
        "summary": "Get workflow status",
        "parameters": [ { "name": "id", "in": "path", "required": true, "schema": { "type": "string" } } ],
        "responses": { "200": { "description": "workflow", "content": { "application/json": { "schema": { "type": "object" } } } }, "404": { "description": "not found" } }
      }
    }
  }
}
