{
  "$schema": "https://raw.githubusercontent.com/RicoSuter/NSwag/master/src/NSwag.Commands/configuration.schema.json",
  "runtime": "Net70",
  "documentGenerator": {
    "fromSwagger": {
      "url": "http://localhost:5002/swagger/v1/swagger.json",
      "output": null
    }
  },
  "codeGenerators": {
    "csharp": {
      "className": "OrchestratorApiClient",
      "namespace": "Generated.Clients.Orchestrator",
      "generateClientInterfaces": true,
      "generateDtoTypes": true,
      "injectHttpClient": true,
      "useBaseUrl": true,
      "output": "Generated/OrchestratorClient.cs"
    },
    "typescript": {
      "template": "fetch",
      "className": "OrchestratorApiClient",
      "moduleName": "orchestrator-client",
      "typeScriptVersion": 5.0,
      "output": "Generated/orchestrator-client.ts"
    }
  }
}
