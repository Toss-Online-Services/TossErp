{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TossErpDb": "Host=localhost;Database=TossErpStock;Username=postgres;Password=password"
  },
  "EventBus": {
    "EventBusConnection": "amqp://guest:guest@localhost:5672",
    "EventBusUserName": "guest",
    "EventBusPassword": "guest",
    "EventBusRetryCount": "5",
    "SubscriptionClientName": "Stock.API"
  },
  "JwtSettings": {
    "SecretKey": "your-secret-key-here-make-it-long-and-secure",
    "Issuer": "TossErp",
    "Audience": "TossErp.Users",
    "ExpirationInMinutes": 60
  }
}
